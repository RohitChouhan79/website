/* ------------------nav starts------------------ */
nav {
    width: 100%;
    height: 10%;
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 10px 7%;
    position: fixed;
    background-color: rgba(250, 250, 250, 0.547);
    z-index: 99;
}

nav > img {
    height: 130px;
}

nav > ul {
    display: flex;
    align-items: center;
    list-style: none;
}
nav > ul > li {
    font-weight: 500;
    margin: 0 15px;
}

nav > ul > li > button {
    padding: 10px 20px;
    border-radius: 5px;
    border: none;
    background-color: #6449f0;
    color: white;
}

.menu-icon {
    display: none;
}

